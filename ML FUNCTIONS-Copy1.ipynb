   "source": [
    "EUCLIDEAN AND MANHATTAN DISTANCES FOR K-NEIGHBOURS"
   ]
  },
  {
   "cell_type": "raw",
   "id": "24dc82b0",
   "metadata": {},
   "source": [
    "Euclidean:\n",
    "\n",
    "def get_euclidean_distance(p1, p2):\n",
    "  dist=0\n",
    "  \n",
    " for i in range(len(p1)):\n",
    "   dist=dist + (p1[i] + p2[i]) **2\n",
    "   \n",
    "   \n",
    "  euclidean_dist= np.sqrt(dist) "
   ]
  },
  {
   "cell_type": "raw",
   "id": "12f0ae83",
   "metadata": {},
   "source": [
    "Manhattan:\n",
    "\n",
    "def get_manhattan_dist(p1, p2)\n",
    "\n",
    "  dist=0\n",
    "  \n",
    "  for i in range(len(p1)):\n",
    "     dist= dist + abs(p1[i] - p2[i])\n",
    "     \n",
    "     manhattan_dist = dist"
   ]
